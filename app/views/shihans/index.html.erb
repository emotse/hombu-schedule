<% @shihans.each do |shihan| %>
  <ul>
  <li><%= link_to "#{shihan.name_en}", shihan_path(shihan) %></li>
    <% shihan.scheduled_classes.each do |scheduled_class| %>
      <ul>
        <li><%= scheduled_class.day %></li>
        <li><%= scheduled_class.time %></li>
        <%= link_to "Class Link", scheduled_class_path(scheduled_class) %>
      </ul>
    <% end %>
  </ul>
<% end %>
